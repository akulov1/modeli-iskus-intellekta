@prefix : <http://example.com/move#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.com/move> rdf:type owl:Ontology .

# КЛАССЫ

:MovePlan rdf:type owl:Class ;
  rdfs:label "План переезда"@ru .

:Task rdf:type owl:Class ; rdfs:label "Задача"@ru .
:PackingTask rdf:type owl:Class ; rdfs:subClassOf :Task ; rdfs:label "Упаковка"@ru .
:BookingTask rdf:type owl:Class ; rdfs:subClassOf :Task ; rdfs:label "Заказ услуги"@ru .
:AdministrativeTask rdf:type owl:Class ; rdfs:subClassOf :Task ; rdfs:label "Административная задача"@ru .
:TransportationTask rdf:type owl:Class ; rdfs:subClassOf :Task ; rdfs:label "Перевозка"@ru .
:CleaningTask rdf:type owl:Class ; rdfs:subClassOf :Task ; rdfs:label "Уборка"@ru .

:Resource rdf:type owl:Class ; rdfs:label "Ресурс"@ru .
:PackingMaterial rdf:type owl:Class ; rdfs:subClassOf :Resource ; rdfs:label "Упаковочный материал"@ru .
:Vehicle rdf:type owl:Class ; rdfs:subClassOf :Resource ; rdfs:label "Транспорт"@ru .
:Equipment rdf:type owl:Class ; rdfs:subClassOf :Resource ; rdfs:label "Оборудование"@ru .

:Item rdf:type owl:Class ; rdfs:label "Предмет"@ru .
:Furniture rdf:type owl:Class ; rdfs:subClassOf :Item ; rdfs:label "Мебель"@ru .
:Appliance rdf:type owl:Class ; rdfs:subClassOf :Item ; rdfs:label "Техника"@ru .

:Location rdf:type owl:Class ; rdfs:label "Адрес"@ru .
:Service rdf:type owl:Class ; rdfs:label "Услуга"@ru .
:Person rdf:type owl:Class ; rdfs:label "Человек"@ru .

# СВОЙСТВА

:hasTask rdf:type owl:ObjectProperty ;
  rdfs:domain :MovePlan ; rdfs:range :Task ;
  rdfs:label "содержит задачу"@ru .

:hasItem rdf:type owl:ObjectProperty ;
  rdfs:domain :MovePlan ; rdfs:range :Item ;
  rdfs:label "содержит предмет"@ru .

:hasOrigin rdf:type owl:ObjectProperty ;
  rdfs:domain :MovePlan ; rdfs:range :Location ;
  rdfs:label "исходный адрес"@ru .

:hasDestination rdf:type owl:ObjectProperty ;
  rdfs:domain :MovePlan ; rdfs:range :Location ;
  rdfs:label "адрес назначения"@ru .

:hasOwner rdf:type owl:ObjectProperty ;
  rdfs:domain :MovePlan ; rdfs:range :Person ;
  rdfs:label "владелец плана"@ru .

:requiresResource rdf:type owl:ObjectProperty ;
  rdfs:domain :Task ; rdfs:range :Resource ;
  rdfs:label "требует ресурс"@ru .

:requiresService rdf:type owl:ObjectProperty ;
  rdfs:domain :Task ; rdfs:range :Service ;
  rdfs:label "требует услугу"@ru .

:precedes rdf:type owl:ObjectProperty ;
  rdfs:domain :Task ; rdfs:range :Task ;
  owl:transitive true ;
  rdfs:label "предшествует"@ru .

:dueDate rdf:type owl:DatatypeProperty ;
  rdfs:domain :Task ; rdfs:range xsd:date ;
  rdfs:label "дата выполнения"@ru .

# ИНДИВИДЫ

:OldHome rdf:type :Location ; rdfs:label "Старая квартира"@ru .
:NewHome rdf:type :Location ; rdfs:label "Новая квартира"@ru .
:Ivanov rdf:type :Person ; rdfs:label "Иванов И.И."@ru .

:MyMove rdf:type :MovePlan ;
  rdfs:label "План переезда Иванова"@ru ;
  :hasOrigin :OldHome ;
  :hasDestination :NewHome ;
  :hasOwner :Ivanov .

:MovingService rdf:type :Service ; rdfs:label "Услуги перевозки"@ru .
:CleaningService rdf:type :Service ; rdfs:label "Клининг"@ru .

:Boxes rdf:type :PackingMaterial ; rdfs:label "Коробки"@ru .
:Tape rdf:type :PackingMaterial ; rdfs:label "Скотч"@ru .
:BubbleWrap rdf:type :PackingMaterial ; rdfs:label "Пленка"@ru .
:Truck rdf:type :Vehicle ; rdfs:label "Грузовик"@ru .
:Dolly rdf:type :Equipment ; rdfs:label "Тележка"@ru .

:Sofa rdf:type :Furniture ; rdfs:label "Диван"@ru .
:Table rdf:type :Furniture ; rdfs:label "Стол"@ru .
:Bed rdf:type :Furniture ; rdfs:label "Кровать"@ru .
:TV rdf:type :Appliance ; rdfs:label "Телевизор"@ru .
:Fridge rdf:type :Appliance ; rdfs:label "Холодильник"@ru .

:TaskInventory rdf:type :AdministrativeTask ;
  rdfs:label "Составить инвентарь"@ru ;
  :dueDate "2025-12-15"^^xsd:date ;
  :precedes :TaskPack .

:TaskBookMovers rdf:type :BookingTask ;
  rdfs:label "Заказать перевозку"@ru ;
  :dueDate "2025-12-20"^^xsd:date ;
  :requiresService :MovingService ;
  :precedes :TaskMove .

:TaskChangeAddress rdf:type :AdministrativeTask ;
  rdfs:label "Оформить смену адреса"@ru ;
  :dueDate "2025-12-22"^^xsd:date ;
  :precedes :TaskMove .

:TaskTransferUtilities rdf:type :AdministrativeTask ;
  rdfs:label "Перевести коммунальные услуги"@ru ;
  :dueDate "2025-12-28"^^xsd:date ;
  :precedes :TaskMove .

:TaskBookCleaning rdf:type :BookingTask ;
  rdfs:label "Заказать клининг"@ru ;
  :dueDate "2026-01-05"^^xsd:date ;
  :requiresService :CleaningService ;
  :precedes :TaskClean .

:TaskPack rdf:type :PackingTask ;
  rdfs:label "Упаковать вещи"@ru ;
  :dueDate "2026-01-09"^^xsd:date ;
  :requiresResource :Boxes , :Tape , :BubbleWrap ;
  :precedes :TaskMove .

:TaskMove rdf:type :TransportationTask ;
  rdfs:label "Переезд"@ru ;
  :dueDate "2026-01-10"^^xsd:date ;
  :requiresResource :Truck , :Dolly ;
  :requiresService :MovingService ;
  :precedes :TaskClean .

:TaskClean rdf:type :CleaningTask ;
  rdfs:label "Уборка старого жилья"@ru ;
  :dueDate "2026-01-11"^^xsd:date ;
  :requiresService :CleaningService .

:MyMove :hasTask
  :TaskInventory , :TaskBookMovers , :TaskChangeAddress ,
  :TaskTransferUtilities , :TaskBookCleaning ,
  :TaskPack , :TaskMove , :TaskClean ;
  :hasItem :Sofa , :Table , :Bed , :TV , :Fridge .
